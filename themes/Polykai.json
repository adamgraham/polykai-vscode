    "tokenColors": [
        {
            "name": "Source",
            "scope": [
                "source",
                "string.template meta",
                "text"
            ],
            "settings": {
                "foreground": "#f8f8f8"
            }
        },
        {
            "name": "Source",
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Source",
            "scope": [
                "markup.bold",
                "strong"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Source",
            "scope": [
                "emphasis",
                "entity.name.variable.parameter",
                "markup.italic",
                "variable.parameter"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#909090"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "entity.name.function.decorator",
                "entity.name.tag",
                "keyword",
                "meta.function.decorator support.type",
                "meta.preprocessor punctuation.separator.hash",
                "punctuation.definition.tag",
                "punctuation.definition.template-expression",
                "storage",
                "string.other.link.title"
            ],
            "settings": {
                "foreground": "#ff0060"
            }
        },
        {
            "name": "String",
            "scope": [
                "markup.quote",
                "markup.quote meta",
                "storage.modifier.import",
                "storage.modifier.package",
                "string"
            ],
            "settings": {
                "foreground": "#ffe080"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.character",
                "constant.language",
                "constant.numeric",
                "constant.other.character-class",
                "constant.other.color",
                "constant.other.placeholder",
                "constant.other.rune.go",
                "constant.others.fsharp",
                "entity.other.keyframe-offset.percentage",
                "keyword.operator.quantifier",
                "keyword.other.unit",
                "markup beginning.punctuation.definition",
                "meta.separator.markdown",
                "storage.type.number"
            ],
            "settings": {
                "foreground": "#c080ff"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant.other",
                "entity.name.enum",
                "entity.name.function.preprocessor",
                "entity.name.variable.enum-member",
                "support.constant",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#6080ff"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "keyword.other.this",
                "storage.type.generic.wildcard",
                "support.variable.dom",
                "support.variable.magic",
                "variable.language"
            ],
            "settings": {
                "foreground": "#6080ff",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Property",
            "scope": [
                "entity.other.attribute-name",
                "markup.fenced_code.block punctuation.definition.markdown",
                "markup.inline.raw",
                "source.batchfile variable.other",
                "source.makefile variable.other",
                "source.shell variable.other",
                "string variable.parameter",
                "support.type.property-name",
                "support.type.propertyname",
                "support.type.vendored.property-name",
                "support.variable"
            ],
            "settings": {
                "foreground": "#ffb000"
            }
        },
        {
            "name": "Type",
            "scope": [
                "entity.other.attribute-name.id",
                "entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element",
                "fenced_code.block.language",
                "keyword.type",
                "markup.fenced_code.block.markdown punctuation.definition.markdown",
                "meta.property-name",
                "support.type"
            ],
            "settings": {
                "foreground": "#ff9020"
            }
        },
        {
            "name": "Type",
            "scope": [
                "storage.type.basic",
                "storage.type.boolean",
                "storage.type.byte",
                "storage.type.c",
                "storage.type.cs",
                "storage.type.error",
                "storage.type.groovy",
                "storage.type.haskell",
                "storage.type.java",
                "storage.type.numeric",
                "storage.type.rune",
                "storage.type.string",
                "storage.type.structured",
                "storage.type.uintptr"
            ],
            "settings": {
                "foreground": "#ff9020"
            }
        },
        {
            "name": "Type",
            "scope": [
                "keyword.type",
                "storage.type.builtin",
                "storage.type.core",
                "storage.type.primitive",
                "support.type.builtin",
                "support.type.core",
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#ff9020",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Class",
            "scope": [
                "entity.name.class",
                "entity.name.type",
                "entity.other.attribute-name.class",
                "entity.other.attribute-name.namespace",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.heading entity",
                "meta.tag.sgml.doctype",
                "storage.class",
                "support.class",
                "support.other.module"
            ],
            "settings": {
                "foreground": "#a0ff20"
            }
        },
        {
            "name": "Class",
            "scope": [
                "storage.class.std",
                "support.class.builtin"
            ],
            "settings": {
                "foreground": "#a0ff20",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Function",
            "scope": [
                "entity.name.function",
                "markup.underline.link",
                "meta.function-call.generic",
                "support.function"
            ],
            "settings": {
                "foreground": "#40c4ff"
            }
        },
        {
            "name": "Special",
            "scope": [
                "meta.definition variable.other.constant",
                "meta.export meta.block variable"
            ],
            "settings": {
                "foreground": "#f8f8f8",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "foreground": "#ff0030",
                "background": "#f8f8f8"
            }
        }
    ]
}